{
    "name": "presta/cms-contact-bundle",
    "type": "symfony-bundle",
    "description": "PrestaCMS Contact bundle : Add contact block to PrestaCMS",
    "keywords": ["symfony2", "bundle", "cms", "symfony-cmf", "prestaconcept"],
    "homepage": "https://github.com/prestaconcept/PrestaCMSContactBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Bastien",
            "email": "nbastien@prestaconcept.net",
            "homepage": "http://www.prestaconcept.net/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2"
    },
    "suggest": {
        "presta/cms-core-bundle": "1.0.*@dev"
    },
    "autoload": {
        "psr-0": { "Presta\\CMSContactBundle": "" }
    },
    "target-dir": "Presta/CMSContactBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
